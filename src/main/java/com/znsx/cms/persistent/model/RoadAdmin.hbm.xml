<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.znsx.cms.persistent.model">
	<class name="RoadAdmin" proxy="com.znsx.cms.persistent.model.RoadAdmin"
		table="em_cooperative_road_admin">
		<cache usage="read-only"/>
		<id name="id" type="string">
			<column name="id" />
			<generator class="uuid" />
		</id>
		<property name="name" type="string">
			<column name="name" />
		</property>
		<property name="standardNumber" type="string">
			<column name="standard_number" />
		</property>
		<property name="location" type="string">
			<column name="location" />
		</property>
		<property name="telephone" type="string">
			<column name="telephone" />
		</property>
		<property name="fax" type="string">
			<column name="fax" />
		</property>
		<property name="email" type="string">
			<column name="email" />
		</property>
		<property name="linkMan" type="string">
			<column name="link_man" />
		</property>
		<property name="mobile" type="string">
			<column name="mobile" />
		</property>
		<property name="carNumber" type="short">
			<column name="car_number" />
		</property>
		<property name="teamNumber" type="short">
			<column name="team_number" />
		</property>
		<property name="longitude" type="string">
			<column name="longitude" />
		</property>
		<property name="latitude" type="string">
			<column name="latitude" />
		</property>
		<property name="note" type="string">
			<column name="note" />
		</property>
	</class>
</hibernate-mapping>
