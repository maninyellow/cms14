<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.znsx.cms.persistent.model">
	<class name="StandardNumber" proxy="com.znsx.cms.persistent.model.StandardNumber"
		table="sv_standard_number">
		<cache usage="read-write"/>
		<id name="id" type="string">
			<column name="id" />
			<generator class="uuid" />
		</id>
		<property name="sn" type="string">
			<column name="standard_number" />
		</property>
		<property name="classType" type="string">
			<column name="class_type" />
		</property>
	</class>
</hibernate-mapping>
