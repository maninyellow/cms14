<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.znsx.cms.persistent.model">
	<class name="CenterWorkRecord" proxy="com.znsx.cms.persistent.model.CenterWorkRecord"
		table="tm_center_work_record">
		<id name="id" type="string">
			<column name="id" />
			<generator class="uuid" />
		</id>
		<property name="userId" type="string">
			<column name="user_id" />
		</property>
		<property name="userName" type="string">
			<column name="user_name" />
		</property>
		<property name="monitorUsers" type="string">
			<column name="monitor_users" />
		</property>
		<property name="maintainUsers" type="string">
			<column name="maintain_users" />
		</property>
		<property name="note" type="string">
			<column name="note" />
		</property>
		<property name="updateTime" type="timestamp">
			<column name="update_time" />
		</property>
		<property name="recordTime" type="timestamp">
			<column name="record_time" />
		</property>
	</class>
</hibernate-mapping>
