<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.znsx.cms.persistent.model">
	<class name="DasGpsReal" proxy="com.znsx.cms.persistent.model.DasGpsReal"
		table="gps_real">
		<id name="id" type="string">
			<column name="id" />
			<generator class="uuid" />
		</id>
		<property name="standardNumber" type="string">
			<column name="standard_number" />
		</property>
		<property name="recTime" type="timestamp">
			<column name="rec_time" />
		</property>
		<property name="longitude" type="string">
			<column name="longitude" />
		</property>
		<property name="latitude" type="string">
			<column name="latitude" />
		</property>
		<property name="bearing" type="string">
			<column name="bearing" />
		</property>
		<property name="speed" type="integer">
			<column name="speed" />
		</property>
		<property name="altitude" type="string">
			<column name="altitude" />
		</property>
		<property name="maxSpeed" type="integer">
			<column name="max_speed" />
		</property>
		<property name="minSpeed" type="integer">
			<column name="min_speed" />
		</property>
		<property name="status" type="short">
			<column name="status" />
		</property>
		<property name="commStatus" type="short">
			<column name="comm_status" />
		</property>
		<property name="reserve" type="string">
			<column name="reserve" />
		</property>
		<property name="organ" type="string">
			<column name="organ" />
		</property>
	</class>
</hibernate-mapping>
