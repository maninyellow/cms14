<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.znsx.cms.persistent.model">
	<class name="SoundRecordLog" proxy="com.znsx.cms.persistent.model.SoundRecordLog"
		table="tm_sound_record_log">
		<cache usage="read-write"/>
		<id name="id" type="string">
			<column name="id" />
			<generator class="uuid" />
		</id>
		<property name="createTime" type="long">
			<column name="create_time" />
		</property>
		<property name="updateTime" type="long">
			<column name="update_time" />
		</property>
		<property name="duration" type="int">
			<column name="duration" />
		</property>
		<property name="callFlag" type="string">
			<column name="call_flag" />
		</property>
		<property name="status" type="string">
			<column name="status" />
		</property>
		<property name="httpAddress" type="string">
			<column name="http_address" />
		</property>
		<property name="note" type="string">
			<column name="note" />
		</property>
		<property name="name" type="string">
			<column name="name" />
		</property>
		<property name="callNumber" type="string">
			<column name="call_number" />
		</property>
		<property name="hostNumber" type="string">
			<column name="host_number" />
		</property>
	</class>
</hibernate-mapping>
