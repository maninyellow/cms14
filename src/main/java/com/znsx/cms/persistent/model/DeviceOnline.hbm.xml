<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.znsx.cms.persistent.model">
	<class name="DeviceOnline" proxy="com.znsx.cms.persistent.model.DeviceOnline"
		table="sv_device_online">
		<id name="id" type="string">
			<column name="id" />
			<generator class="uuid" />
		</id>
		<property name="standardNumber" type="string">
			<column name="standard_number" />
		</property>
		<property name="onlineTime" type="long">
			<column name="online_time" />
		</property>
		<property name="offlineTime" type="long">
			<column name="offline_time" />
		</property>
	</class>
</hibernate-mapping>
